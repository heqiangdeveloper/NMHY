<android.support.design.widget.CoordinatorLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:fitsSystemWindows="true">

    <include layout="@layout/activity_settings_title"/>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_behavior="@string/appbar_scrolling_view_behavior">
        <RelativeLayout
            android:id="@+id/empty_rl"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <ImageView
                android:id="@+id/empty_iv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@mipmap/ic_empty"
                android:layout_centerInParent="true"/>
            <TextView
                android:text="@string/no_data_warn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:layout_below="@id/empty_iv"/>
        </RelativeLayout>
        <!--待报-->
        <ScrollView
            android:id="@+id/content_sv"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:background="#f2f2f2"
            android:scrollbars="none"
            android:layout_marginBottom="0dp"
            android:visibility="visible"
            app:layout_behavior="@string/appbar_scrolling_view_behavior">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:focusable="true"
                android:focusableInTouchMode="true"
                android:orientation="vertical">


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:focusable="true"
                    android:focusableInTouchMode="true"
                    android:layout_marginLeft="10dip"
                    android:layout_marginRight="10dip"
                    android:orientation="vertical">

                    <LinearLayout
                        android:layout_marginTop="20dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_weight="1.0"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="2dip"
                            android:background="@color/white"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="right"
                                android:padding="8dip"
                                android:text="@string/portName_label"
                                android:textColor="@color/main_title_text_color"
                                android:textSize="@dimen/main_text_size"/>

                            <TextView
                                android:id="@+id/portName_tv"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="right"
                                android:padding="8dip"
                                android:text="123123"
                                android:textColor="@color/main_value_text_color"
                                android:textSize="@dimen/main_text_size"/>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_weight="1.0"
                            android:layout_height="match_parent"
                            android:layout_marginTop="2dip"
                            android:background="@color/white"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="right"
                                android:padding="8dip"
                                android:text="@string/jobTypeValue_label"
                                android:textColor="@color/main_title_text_color"
                                android:textSize="@dimen/main_text_size"/>

                            <TextView
                                android:id="@+id/jobTypeValue_tv"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:gravity="right"
                                android:padding="8dip"
                                android:text="123123"
                                android:textColor="@color/main_value_text_color"
                                android:textSize="@dimen/main_text_size"/>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_weight="1.0"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="2dip"
                            android:background="@color/white"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="right"
                                android:padding="8dip"
                                android:text="@string/voyageStatusDesc_label"
                                android:textColor="@color/main_title_text_color"
                                android:textSize="@dimen/main_text_size"/>

                            <TextView
                                android:id="@+id/voyageStatusDesc_tv"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:background="@color/white"
                                android:gravity="right"
                                android:inputType="numberDecimal"
                                android:maxLength="11"
                                android:padding="8dip"
                                android:drawablePadding="@dimen/drawable_padding_size"
                                android:textColor="@color/main_value_text_color"
                                android:textSize="@dimen/main_text_size"/>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_weight="1.0"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="2dip"
                            android:background="@color/white"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="right"
                                android:padding="8dip"
                                android:text="@string/reason_label"
                                android:textColor="@color/main_title_text_color"
                                android:textSize="@dimen/main_text_size"/>

                            <EditText
                                android:id="@+id/reason_tv"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:background="@color/white"
                                android:gravity="right"
                                android:padding="8dip"
                                android:hint="@string/reason_hint"
                                android:drawablePadding="@dimen/drawable_padding_size"
                                android:textColor="@color/main_value_text_color"
                                android:textSize="@dimen/main_text_size"/>
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_weight="1.0"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="2dip"
                            android:background="@color/white"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="right"
                                android:padding="8dip"
                                android:text="@string/estimatedTime_label"
                                android:textColor="@color/main_title_text_color"
                                android:textSize="@dimen/main_text_size"/>

                            <TextView
                                android:id="@+id/estimatedTime_tv"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:background="@color/white"
                                android:gravity="right"
                                android:padding="8dip"
                                android:text=""
                                android:textColor="@color/main_value_text_color"
                                android:textSize="@dimen/main_text_size"/>

                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_weight="1.0"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="2dip"
                            android:background="@color/white"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="right"
                                android:padding="8dip"
                                android:text="@string/occurTime_label"
                                android:textColor="@color/main_title_text_color"
                                android:textSize="@dimen/main_text_size"/>

                            <TextView
                                android:id="@+id/occurTime_tv"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:background="@color/white"
                                android:gravity="right"
                                android:padding="8dip"
                                android:textColor="@color/main_value_text_color"
                                android:textSize="@dimen/main_text_size"/>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_weight="1.0"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="2dip"
                            android:background="@color/white"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="right"
                                android:padding="8dip"
                                android:text="@string/reportTime_label"
                                android:textColor="@color/main_title_text_color"
                                android:textSize="@dimen/main_text_size"/>

                            <TextView
                                android:id="@+id/reportTime_tv"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:background="@color/white"
                                android:gravity="right"
                                android:padding="8dip"
                                android:textColor="@color/main_value_text_color"
                                android:textSize="@dimen/main_text_size"/>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_weight="1.0"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="2dip"
                            android:background="@color/white"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="right"
                                android:padding="8dip"
                                android:text="@string/location_label"
                                android:textColor="@color/main_title_text_color"
                                android:textSize="@dimen/main_text_size"/>

                            <TextView
                                android:id="@+id/location_tv"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:background="@color/white"
                                android:gravity="right"
                                android:padding="8dip"
                                android:hint="@string/location_hint_label"
                                android:drawableRight="@mipmap/icon_location"
                                android:textColor="@color/main_value_text_color"
                                android:textSize="@dimen/main_text_size"/>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_weight="1.0"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="2dip"
                            android:background="@color/white"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="right"
                                android:padding="8dip"
                                android:text="@string/longitude_label"
                                android:textColor="@color/main_title_text_color"
                                android:textSize="@dimen/main_text_size"/>

                            <TextView
                                android:id="@+id/longitude_tv"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:background="@color/white"
                                android:gravity="right"
                                android:padding="8dip"
                                android:textColor="@color/main_value_text_color"
                                android:textSize="@dimen/main_text_size"/>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_weight="1.0"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="2dip"
                            android:background="@color/white"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="right"
                                android:padding="8dip"
                                android:text="@string/latitude_label"
                                android:textColor="@color/main_title_text_color"
                                android:textSize="@dimen/main_text_size"/>

                            <TextView
                                android:id="@+id/latitude_tv"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:background="@color/white"
                                android:gravity="right"
                                android:padding="8dip"
                                android:textColor="@color/main_value_text_color"
                                android:textSize="@dimen/main_text_size"/>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_weight="1.0"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="2dip"
                            android:background="@color/white"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="right"
                                android:padding="8dip"
                                android:text="@string/speed_label"
                                android:textColor="@color/main_title_text_color"
                                android:textSize="@dimen/main_text_size"/>

                            <EditText
                                android:id="@+id/speed_tv"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:background="@color/white"
                                android:gravity="right"
                                android:padding="8dip"
                                android:inputType="numberDecimal"
                                android:hint="@string/speed_hint"
                                android:textColor="@color/main_value_text_color"
                                android:textSize="@dimen/main_text_size"/>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_weight="1.0"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="2dip"
                            android:background="@color/white"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="right"
                                android:padding="8dip"
                                android:text="@string/weather_label"
                                android:textColor="@color/main_title_text_color"
                                android:textSize="@dimen/main_text_size"/>

                            <EditText
                                android:id="@+id/weather_tv"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:background="@color/white"
                                android:gravity="right"
                                android:padding="8dip"
                                android:hint="@string/weather_hint"
                                android:textColor="@color/main_value_text_color"
                                android:textSize="@dimen/main_text_size"/>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_weight="1.0"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="2dip"
                            android:background="@color/white"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="right"
                                android:padding="8dip"
                                android:text="@string/feedback_label"
                                android:textColor="@color/main_title_text_color"
                                android:textSize="@dimen/main_text_size"/>

                            <EditText
                                android:id="@+id/feedback_tv"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:background="@color/white"
                                android:gravity="right"
                                android:padding="8dip"
                                android:hint="@string/feedback_hint"
                                android:textColor="@color/main_value_text_color"
                                android:textSize="@dimen/main_text_size"/>
                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <LinearLayout
                            android:layout_width="0dp"
                            android:layout_weight="1.0"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="2dip"
                            android:background="@color/white"
                            android:orientation="horizontal">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:gravity="right"
                                android:padding="8dip"
                                android:text="@string/remark_label"
                                android:textColor="@color/main_title_text_color"
                                android:textSize="@dimen/main_text_size"/>

                            <EditText
                                android:id="@+id/remark_tv"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:background="@color/white"
                                android:gravity="right"
                                android:padding="8dip"
                                android:hint="@string/remark_hint"
                                android:textColor="@color/main_value_text_color"
                                android:textSize="@dimen/main_text_size"/>
                        </LinearLayout>
                    </LinearLayout>

                    <TextView
                        android:drawableLeft="@mipmap/ic_barge_cabin"
                        android:drawablePadding="10dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="@dimen/main_text_size"
                        android:padding="8dip"
                        android:layout_alignParentLeft="true"
                        android:text="@string/add_oil_report_detail_label2"
                        android:textColor="@color/colorPrimary"/>
                    <!--燃油信息-->
                    <TableLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:stretchColumns="0,1,2"
                        android:background="#000000"
                        android:layout_marginLeft="10dip"
                        android:layout_marginRight="10dip">
                        <TableRow>
                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/add_oil_table_title_height"
                                android:layout_margin="0.5dp"
                                android:background="@color/colorPrimaryDark"
                                android:gravity="center"
                                android:textSize="@dimen/main_text_size"
                                android:textColor="@color/white"
                                android:text="@string/fuelKind_label" />
                            <!--单位 不需要-->
                            <TextView
                                android:gravity="center"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_margin="0.5dp"
                                android:background="@color/colorPrimaryDark"
                                android:textSize="@dimen/text_size_14"
                                android:textColor="@color/white"
                                android:visibility="gone"
                                android:text="@string/unit_label" />

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1.5"
                                android:background="@color/colorPrimaryDark"
                                android:layout_margin="0.5dp"
                                android:gravity="center"
                                android:text="@string/realStoreQty_label"
                                android:textColor="@color/white"
                                android:textSize="@dimen/main_text_size"/>
                        </TableRow>
                        <TableRow>
                            <TextView
                                android:gravity="center"
                                android:id="@+id/oilType1_tv"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/add_oil_table_item_height"
                                android:layout_margin="0.5dp"
                                android:background="#ffffffff"
                                android:textSize="@dimen/main_text_size"
                                android:textColor="@color/main_title_text_color"
                                android:text="@string/type_heavy_oil" />
                            <TextView
                                android:gravity="center"
                                android:id="@+id/oilUnit1_tv"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_margin="0.5dp"
                                android:background="#ffffffff"
                                android:textSize="@dimen/text_size_14"
                                android:visibility="gone"
                                android:textColor="@color/main_value_text_color"
                                android:text="" />
                            <EditText
                                android:gravity="center"
                                android:id="@+id/oilAmount1_et"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1.5"
                                android:layout_margin="0.5dp"
                                android:background="#ffffffff"
                                android:inputType="numberDecimal"
                                android:textSize="@dimen/main_text_size"
                                android:textColor="@color/main_value_text_color"
                                android:focusable="true"
                                android:focusableInTouchMode="true"
                                android:hint="@string/add_oil_amount_hint_label"/>
                        </TableRow>
                        <TableRow>
                            <TextView
                                android:gravity="center"
                                android:id="@+id/oilType2_tv"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/add_oil_table_item_height"
                                android:layout_margin="0.5dp"
                                android:background="#ffffffff"
                                android:textSize="@dimen/main_text_size"
                                android:textColor="@color/main_title_text_color"
                                android:text="@string/type_light_oil" />
                            <TextView
                                android:gravity="center"
                                android:id="@+id/oilUnit2_tv"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_margin="0.5dp"
                                android:background="#ffffffff"
                                android:textSize="@dimen/text_size_14"
                                android:visibility="gone"
                                android:text="" />
                            <EditText
                                android:gravity="center"
                                android:id="@+id/oilAmount2_et"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1.5"
                                android:layout_margin="0.5dp"
                                android:background="#ffffffff"
                                android:inputType="numberDecimal"
                                android:textSize="@dimen/main_text_size"
                                android:textColor="@color/main_value_text_color"
                                android:hint="@string/add_oil_amount_hint_label"/>
                        </TableRow>
                        <TableRow>
                            <TextView
                                android:gravity="center"
                                android:id="@+id/oilType3_tv"
                                android:layout_width="match_parent"
                                android:layout_height="@dimen/add_oil_table_item_height"
                                android:layout_margin="0.5dp"
                                android:background="#ffffffff"
                                android:textSize="@dimen/main_text_size"
                                android:textColor="@color/main_title_text_color"
                                android:text="@string/type_machine_oil" />
                            <TextView
                                android:gravity="center"
                                android:id="@+id/oilUnit3_tv"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_margin="0.5dp"
                                android:background="#ffffffff"
                                android:visibility="gone"
                                android:textSize="@dimen/text_size_14"
                                android:text="" />
                            <EditText
                                android:gravity="center"
                                android:id="@+id/oilAmount3_et"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:layout_weight="1.5"
                                android:layout_margin="0.5dp"
                                android:background="#ffffffff"
                                android:inputType="numberDecimal"
                                android:textSize="@dimen/main_text_size"
                                android:textColor="@color/main_value_text_color"
                                android:hint="@string/add_oil_amount_hint_label"/>
                        </TableRow>
                        <!--防止第2列的最下面出现浅色-->
                        <TableRow>
                            <TextView
                                android:gravity="center"
                                android:layout_width="match_parent"
                                android:layout_height="0dp"
                                android:layout_margin="0.5dp"
                                android:background="#ffffffff"
                                android:text="" />
                        </TableRow>
                    </TableLayout>
                </LinearLayout>
                <Button
                    android:layout_marginLeft="10dip"
                    android:layout_marginRight="10dip"
                    android:id="@+id/commit_bt"
                    android:layout_marginTop="40dp"
                    android:layout_marginBottom="40dp"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/btn_commit_label"
                    android:textColor="@color/white"
                    android:layout_gravity="bottom"
                    android:background="@drawable/shape_login_button_off"/>
            </LinearLayout>
        </ScrollView>
        <!--已报-->
        <android.support.design.widget.CoordinatorLayout
            android:id="@+id/content_cl"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="5dip"
            android:layout_marginTop="5dp"
            android:background="@android:color/white"
            android:visibility="gone"
            app:layout_behavior="@string/appbar_scrolling_view_behavior">

            <android.support.v4.widget.SwipeRefreshLayout
                android:id="@+id/swipeRefreshLayout"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                app:layout_behavior="@string/appbar_scrolling_view_behavior">

                <android.support.v7.widget.RecyclerView
                    android:id="@+id/recyclerView"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:scrollbars="vertical" />
            </android.support.v4.widget.SwipeRefreshLayout>
        </android.support.design.widget.CoordinatorLayout>
    </FrameLayout>
    <!--more-->
    <LinearLayout
        android:id="@+id/popup_menu_layout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginTop="?actionBarSize"
        android:layout_marginRight="10dp"
        android:elevation="20dp"
        android:visibility="gone"
        android:translationZ="20dp"
        android:background="@color/white"
        android:layout_gravity="end|top">
        <include layout="@layout/more_popup_menu"/>
    </LinearLayout>
</android.support.design.widget.CoordinatorLayout>
